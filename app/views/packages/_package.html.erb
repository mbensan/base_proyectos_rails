<div id="<%= dom_id package %>">
  <p>
    <strong>Name:</strong>
    <%= package.name %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= package.price %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= package.duration %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= package.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= package.end_date %>
  </p>

  <p>
    <strong>Destination:</strong>
    <%= package.destination_id %>
  </p>

</div>
